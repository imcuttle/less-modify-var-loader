  "license": "MIT",
  "repository": "imcuttle/less-modify-var-loader",
  "jest": {
    "testMatch": [
      "**/__test{s,}__/*.(spec|test).{t,j}s{x,}"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^7.5.0",
    "@commitlint/config-conventional": "^7.5.0",
    "@types/jest": "^24.0.0",
    "conventional-changelog-cli": "^2.0.11",
    "css-loader": "^2.1.0",
    "husky": "^1.3.1",
    "jest": "^29.3.1",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "memory-fs": "^0.4.1",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0",
    "raw-loader": "^1.0.0",
    "webpack": "^4.29.3"
  },
