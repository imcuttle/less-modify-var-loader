  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "sideEffects": false,
  "engines": {
    "node": ">=8"
  },
  "keywords": [
    "imcuttle",
    "less",
    "modify",
    "var",
    "loader",
    "less-modify-var-loader"
