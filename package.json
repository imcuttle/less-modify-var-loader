{
  "name": "less-modify-var-loader",
  "version": "1.0.0",
  "main": "index.js",
  "description": "The webpack loader for better replace less variables",
  "author": "imcuttle <moyuyc95@gmail.com>",
  "scripts": {
    "test": "jest --detectOpenHandles",
